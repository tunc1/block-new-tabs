{
    "name": "Block new Tabs",
    "description": "Preventing unwanted tabs from opening",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": ["tabs","activeTab"],
    "background":
    {
        "scripts":["background.js"]
    },
	"browser_action": {
        "default_popup": "popup.html"
    }
}